//Blinking LED

const int LED_Pin = 4;

const int Time_Delay = 1000;

void setup()
{
  pinMode(LED_Pin, OUTPUT);
}

void loop()
{
  digitalWrite(LED_Pin, HIGH);
  delay(Time_Delay);

  digitalWrite(LED_Pin, LOW);
  delay(Time_Delay);
}

/*
Connections -
220 Ω Resistor  - GPIO Pin 4 (D4)
LED Anode       - 220 Ω Resistor
LED Cathode     - Ground
*/
